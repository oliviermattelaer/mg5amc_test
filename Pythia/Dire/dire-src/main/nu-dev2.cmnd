
# Number of events to generate.
Main:numberOfEvents       = 1000000
Next:numberCount          = 1000

# Use external PDF sets from LHAPDF for incoming hadrons.
#PDF:useHard               = on
#PDF:pSet                  = LHAPDF6:CT10nlo 
#PDF:pHardSet              = LHAPDF6:CT10nlo 

# No distribution for incoming leptons.
PDF:lepton                = off

# Set reference value of alphaS(Mz).
SpaceShower:alphaSvalue   = 0.1201
TimeShower:alphaSvalue    = 0.1201

# Set hard process.
WeakBosonExchange:ff2ff(t:W) = on
#WeakBosonExchange:ff2ff(t:gmZ) = on
Beams:idA                 = 12
Beams:idB                 = 2212
#Beams:frameType           = 2
#Beams:eA                  = 100.0
#Beams:eB                  = 100.0
Beams:eCM                 = 200.0
PhaseSpace:Q2min          = 2500.
PhaseSpace:mHatMin = 0.

SigmaProcess:alphaEMorder = 0
SigmaProcess:renormScale2 = 5
SigmaProcess:factorScale2 = 5
SigmaProcess:renormFixScale = 8315.251344
SigmaProcess:factorFixScale = 8315.251344
SpaceShower:useFixedFacScale = on
SpaceShower:fixedFacScale = 91.188
TimeShower:useFixedFacScale = on
TimeShower:fixedFacScale = 91.188
PDFinProcess:nQuarkIn = 1

#Enhance:fsr_qed_1->1&22_CS = 50
#Enhance:fsr_qed_1->22&1_CS = 50
#Enhance:fsr_qed_11->11&22_CS = 50
#Enhance:fsr_qed_11->22&11_CS = 50
#Enhance:isr_qed_1->1&22_CS = 50
#Enhance:isr_qed_11->11&22_CS = 50

# Merging and Madgraph settings.
#Dire:doMECs                     = on
#Dire:MG5card                    = param_card_sm.dat
#Merging:doPTLundMerging         = on
#Merging:Process                 = vep>e-j
#Merging:useShowerPlugin         = on
#Merging:TMS                     = 1000000
#Dire:doMerging                  = on
#Merging:applyVeto               = off
#Merging:includeWeightInXsection = off
#Merging:mayRemoveDecayProducts  = on

TimeShower:pTminChgQ = 1
TimeShower:pTminChgL = 1
SpaceShower:pTminChgQ = 1
SpaceShower:pTminChgL = 1

# Use masses of PDF sets also for shower evolution.
# Note: Only correctly handled when using external PDF sets from LHAPDF
ShowerPDF:usePDFalphas    = off
ShowerPDF:useSummedPDF    = off
ShowerPDF:usePDFmasses    = off
DireSpace:forceMassiveMap = on

